{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "clog",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}"
    },
    {
      "name": "snippets",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "git",
        "tag",
        "tidy"
      ]
    },
    {
      "name": "jumbo",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "Jumbo",
        "-C",
        "js",
        "golang"
      ]
    },
    {
      "name": "log",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "Log",
        "-B",
        "1",
        "P",
        "OK message",
        "ERR message"
      ]
    }
  ]
}